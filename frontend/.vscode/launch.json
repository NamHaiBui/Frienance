{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        
        {
            "name": "frienance (Chrome with macros)",
            "request": "launch",
            "type": "dart",
            "args": [
                "--enable-experiment=macros",
                "-d",
                "chrome"
            ]
        },
        {
            "name": "frienance",
            "request": "launch",
            "type": "dart"
        },
        {
            "name": "frienance (profile mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "profile"
        },
        {
            "name": "frienance (release mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "release"
        }
    ]
}